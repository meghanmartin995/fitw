function replacePosts (innerHTML) {
  const posts = document.getElementById('posts');
  posts.innerHTML = innerHTML;
}
// replacePosts("<%= j render 'post', posts: @posts %>");


if ('<%= @filter %>' === "[]") {
replacePosts("<%= j render 'post', posts: Post.all %>");
}
else {
replacePosts("<%= j render 'post', posts: @posts %>");
}

